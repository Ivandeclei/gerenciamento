name: dockercompose2009299629702939399
services:
  gerenciamentoapplication:
    build:
      context: C:\Users\ivand\OneDrive\Documentos\Projetos\gerenciamento\GerenciamentoApplication
      dockerfile: GerenciamentoApplication/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      DB_CONNECTION: sqlserver://db/volumesql
      Database: gerenciamento
      DbPort: "1433"
      DbServer: mssql-server
      DbUser: SA
      Password: SuperPassword@123
    image: gerenciamentoapplication
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\ivand\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ivand\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  mssql-server:
    container_name: sql-server-db
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: SuperPassword@123
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: volume
        source: sqlserver
        target: /var/opt/mssql/data
        volume: {}
networks:
  default:
    name: dockercompose2009299629702939399_default
volumes:
  sqlserver:
    name: dockercompose2009299629702939399_sqlserver